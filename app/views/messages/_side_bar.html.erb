<div class="side_bar">
  <div class="side_bar_top">
    <div class="user_name">
      <%= link_to current_user.name, edit_user_path(current_user) %>
    </div>
    <div class="creat_chat_buttom">チャットを作成する</div>
  </div>
  <div class="chat_name">
    <div class="techroom1">
      <% current_user.rooms.each do |room| %>
        <div class="room">
          <div class="room-name">
            <%= link_to room.name, room_messages_path(room) %>
          </div>
        </div>
      <% end %>
    
    </div>
  </div>
</div>
